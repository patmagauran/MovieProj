{
  "configurations": [
    {
      "type": "node-terminal",
      "name": "Run Script: server",
      "request": "launch",
      "command": "yarn run server",
      "cwd": "${workspaceFolder}",
            "outFiles": [
              "${workspaceFolder}/packages/server/build/**/*.js"
            ]
    },
        {
            "name": "Chrome",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:3000",
            "webRoot": "${workspaceRoot}/src"
        }
  ]
}
